<html>
  <head>
    <title>Mini-Message Board</title>
  </head>
  <body>

    <h1><%= title %></h1>

    <ul>
        <% messages.map((message) => { %>
            <li>
                <p><%= message.text %></p>
                <p><%= message.user %></p>
                <p><%= message.added %></p>
            </li>
        <% }) %>
    </ul>
  </body>
</html>
